\par \pard\plain \ltrpar\s3\cf0{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\sa120\rtlch\af9\afs24\lang1081\ltrch\dbch\af7\langfe2052\hich\f0\fs24\lang1033\loch\f0\fs24\lang1033 
